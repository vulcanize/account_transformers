{
  "name": "@graphile/plugin-supporter",
  "version": "0.4.1",
  "private": true,
  "description": "Adds more features to PostGraphile",
  "main": "dist/index.js",
  "scripts": {
    "prerelease": "npm run test && ([ -x '../../.env_prod' ] && . ../../.env_prod; ./scripts/tag-version postgraphile-supporter)",
    "release": "echo 'Releasing...'",
    "postrelease": "[ -x '../../.env_prod' ] && . ../../.env_prod; ./scripts/ship postgraphile-supporter",
    "prepack": "rm -Rf dist; webpack --mode=production",
    "watch": "webpack --mode=development --watch",
    "test": "jest"
  },
  "keywords": [
    "postgraphile",
    "plugin",
    "graphile-build",
    "graphql",
    "subscriptions",
    "postgresql",
    "pg",
    "postgres"
  ],
  "author": "Benjie Gillam <benjie@graphile.org>",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "debug": "^3.1.0",
    "graphql-subscriptions": "^0.5.8",
    "lodash": ">=4 <5",
    "subscriptions-transport-ws": "^0.9.13"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.53",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0-beta.53",
    "@types/debug": "0.0.30",
    "@types/graphql": "0.13.0",
    "@types/pg": "7.4.5",
    "@types/ws": "4.0.2",
    "babel-eslint": "8.2.2",
    "babel-loader": "8.0.0-beta.4",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-jest": "21.15.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint_d": "5.3.0",
    "inquirer": "5.2.0",
    "jest": "22.4.3",
    "mock-req": "0.2.0",
    "mock-res": "0.5.0",
    "postgraphile": "4.0.0-rc.3",
    "prettier": "1.11.1",
    "semver": "5.5.0",
    "webpack": "4.5.0",
    "webpack-cli": "2.0.14"
  },
  "files": [
    "dist",
    "LICENSE.md",
    "USAGE.md"
  ],
  "jest": {
    "testRegex": "__tests__/.*\\.test\\.js$",
    "modulePathIgnorePatterns": [
      "<rootDir>/release/"
    ]
  }
}
